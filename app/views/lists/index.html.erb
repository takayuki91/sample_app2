<h1>Books</h1>
<% @lists.each do |list| %>

  <p>title</p>
  <span>
    <!--名前つきRoutingにする前-->
    <!--<%#= link_to list.title,"/lists/#{list.id}" %>-->
    <!--名前つきRoutingにした後-->
    <%= link_to list.title,list_path(list.id) %>
  </span>

  <p>body</p>
  <span>
    <%= link_to list.body,list_path(list.id) %>
  </span>
  
  <p>image</p>
  <%= image_tag list.image,size: "200×200" %>

<% end %>